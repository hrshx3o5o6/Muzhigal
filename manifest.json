{
    "manifest_version": 3, 
    "name":"Music transfer Extension", 
    "version": 1, 
    "description": "Transferring liked songs from apple music to spotify and vice versa", 
    "persmissions": [
        "storage", 
        "activeTab"
    ], 
    "host_persmissions" : [
        "http://localhost:3000/*"
    ],
    "action": {
        "default_popup": "popup.html", 
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }, 
    "background": {
        "service_worker": "background.js"
    }
}